/* typec0 */
&vbus5v0_typec_pwr_en {
	status = "okay";
	regulator-min-microvolt = <5000000>;
	regulator-max-microvolt = <5000000>;
	gpios = <&gpio4 RK_PA7 GPIO_ACTIVE_HIGH>;   ///GPIO4_A7

	vin-supply = <&vcc5v0_usb>;
	pinctrl-names = "default";
	pinctrl-0 = <&vbus5v0_typec_pwr_en_gpio>;
};

&gmac1{
 	snps,reset-gpio = <&gpio3 RK_PB7 GPIO_ACTIVE_LOW>;
	phy-mode = "rgmii";
	tx_delay = <0x45>;
	rx_delay = <0x34>;
	status = "okay";
};

&spi1 {
	status = "okay";
	max-freq = <48000000>;
	dev-port = <0>;
	pinctrl-0 = <&spi1m2_pins>;
	num-cs = <1>;

	spi_wk2xxx_2: spi_wk2xxx_2@0{
		status = "okay";
		compatible = "firefly,spi-wk2xxx";
		reg = <0x00>;
		index_base = <5>;
		spi-max-frequency = <10000000>;
		//power-gpio = <&gpio2 4 GPIO_ACTIVE_HIGH>;

		reset-gpio = <&gpio1 RK_PC4 GPIO_ACTIVE_HIGH>;		//WK2124_RST_2	GPIO1_C4
		irq-gpio = <&gpio1 RK_PC3 IRQ_TYPE_EDGE_FALLING>;	//WK2124_INT_2	GPIO1_C3
		cs-gpio = <&gpio1 RK_PD3 GPIO_ACTIVE_HIGH>;			//SPI1_CS0_M2	GPIO1_D3

		pinctrl-0 = <&wk2xxx_2_irq_gpios>;
		pinctrl-names = "default";
	};
};

/* PMIC spi2 */
&spi2 {
	pinctrl-names = "default";
	pinctrl-0 = <&spi2m2_cs0 &spi2m2_pins>;
	num-cs = <1>;
};

&spi4 {
	status = "okay";
	max-freq = <48000000>;
	dev-port = <0>;
	pinctrl-0 = <&spi4m2_pins>;
	num-cs = <1>;

	spi_wk2xxx_1: wk2xxx_spi@0{
		status = "okay";
		compatible = "firefly,spi-wk2xxx";
		reg = <0x00>;
		index_base = <1>;
		spi-max-frequency = <10000000>;
		reset-gpio = <&gpio1 RK_PD5 GPIO_ACTIVE_HIGH>;		//WK2124_RST_1	GPIO1_D5
		irq-gpio = <&gpio1 RK_PB2 IRQ_TYPE_EDGE_FALLING>;	//WK2124_INT_1	GPIO1_B2
		cs-gpio = <&gpio1 RK_PA3 GPIO_ACTIVE_HIGH>;			//SPI4_CS0_M2	GPIO1_A3

		//pinctrl-0 = <&reset_gpios>,<&irq_gpios>,<&cs_gpios>;
		//pinctrl-names = "default";
		pinctrl-0 = <&wk2xxx_1_irq_gpios>;
		pinctrl-names = "default";
	};
};

&sdmmc {
	status = "disabled";
	vmmc-supply = <&vcc_3v3_sd_s0>;
};

&i2c2 {
	status = "disabled";
	pinctrl-names = "default";
	pinctrl-0 = <&i2c2m0_xfer>;
};

&i2c4 {
	status = "disabled";
	pinctrl-names = "default";
	pinctrl-0 = <&i2c4m3_xfer>;
};


&i2c3 {
	status = "okay";
	clock-frequency = <400000>;
	pinctrl-names = "default";
	pinctrl-0 = <&i2c3m4_xfer>;

	sub_core_pca9555: gpio@20 {
		status = "okay";
		compatible = "nxp,pca9555";
		reg = <0x20>;
		gpio-controller;
		#gpio-cells = <2>;
		gpio-group-num = <250>;
    };
};


&vccio_sd_s0 {
    regulator-min-microvolt = <3300000>;
    regulator-max-microvolt = <3300000>;
    //regulator-name = "vccio_sd_s0";
	regulator-name = "vccio_sub_core_pca9555_s0";
};


// &i2c5 {
// 	status = "okay";
// 	pinctrl-names = "default";
// 	pinctrl-0 = <&i2c5m1_xfer>;
// };

&i2c6 {
	status = "okay";
	clock-frequency = <400000>;
   	pinctrl-names = "default";
	pinctrl-0 = <&i2c6m3_xfer>;

	hym8563: hym8563@51 {
		compatible = "haoyu,hym8563";
		reg = <0x51>;
		#clock-cells = <0>;
		clock-frequency = <32768>;
		clock-output-names = "hym8563";
		pinctrl-names = "default";
		pinctrl-0 = <&hym8563_int>;
		interrupt-parent = <&gpio0>;
		interrupts = <RK_PB0 IRQ_TYPE_LEVEL_LOW>;
		wakeup-source;
		status = "okay";
	};

	pc9202: pc9202@3c {
		status = "okay";
		compatible = "firefly,pc9202";
		reg = <0x3c>;
		wd-en-gpio = <&gpio4 RK_PB2 GPIO_ACTIVE_HIGH>;         //GPIO4_B2
		pinctrl-names = "default";
		pinctrl-0 = <&wdt_ctl>;
	};


	pca9555: gpio@21 {
               status = "disabled";
               compatible = "nxp,pca9555";
               reg = <0x21>;
               gpio-controller;
               #gpio-cells = <2>;
               gpio-group-num = <200>;
       };


	usbc0: fusb302@22 {
		compatible = "fcs,fusb302";
		reg = <0x22>;
		interrupt-parent = <&gpio3>;
		interrupts = <RK_PC4 IRQ_TYPE_LEVEL_LOW>;
		pinctrl-names = "default";
		pinctrl-0 = <&usbc0_int>;
		vbus-supply = <&vbus5v0_typec_pwr_en>;
		status = "okay";

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;
				usbc0_role_sw: endpoint@0 {
					remote-endpoint = <&dwc3_0_role_switch>;
				};
			};
		};

		usb_con: connector {
			compatible = "usb-c-connector";
			label = "USB-C";
			data-role = "dual";
			power-role = "dual";
			try-power-role = "sink";
			op-sink-microwatt = <1000000>;
			sink-pdos =
				<PDO_FIXED(5000, 1000, PDO_FIXED_USB_COMM)>;
			source-pdos =
				<PDO_FIXED(5000, 3000, PDO_FIXED_USB_COMM)>;

			altmodes {
				#address-cells = <1>;
				#size-cells = <0>;

				altmode@0 {
					reg = <0>;
					svid = <0xff01>;
					vdo = <0xffffffff>;
				};
			};

			ports {
				#address-cells = <1>;
				#size-cells = <0>;

				port@0 {
					reg = <0>;
					usbc0_orien_sw: endpoint {
						remote-endpoint = <&usbdp_phy0_orientation_switch>;
					};
				};

				port@1 {
					reg = <1>;
					dp_altmode_mux: endpoint {
						remote-endpoint = <&usbdp_phy0_dp_altmode_mux>;
					};
				};
			};
		};
	};

};

